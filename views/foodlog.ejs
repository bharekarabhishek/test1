									        <td>Solid Fats</td>
									        <td>{{solid_fats}} g</td>
									      </tr>
									      
									      <tr class="success">
									        <td>Added Sugar</td>
									        <td>{{added_sugar}} g</td>
									      </tr>
									      
									      <tr class="success">
									        <td>Saturated Fats</td>
									        <td>{{saturated_fats}} g</td>
									      </tr>
									      
									      <tr class="success">
									        <td>Calories</td>
									        <td>{{food_calories}}</td>
									      </tr>
									      
									      
									    </tbody>
									    
	  								</table>
		                		 	
		                		</div>
	                		
	                		</div>
	                		
	                	</div>
	                	
